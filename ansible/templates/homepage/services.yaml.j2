# services
- Media:
    - Calendar:
        icon: mdi-calendar
        href: "#"
        description: Release Calendar
        widget:
            type: calendar
            firstDayInWeek: sunday
            view: agenda
            timezone: Pacific/Honolulu
            maxEvents: 12
            integrations:
                - type: sonarr
                  service_group: Downloads
                  service_name: Sonarr
                  color: blue
                - type: radarr
                  service_group: Downloads
                  service_name: Radarr
                  color: red
    - Plex:
        icon: plex.png
        href: "#"
        description: Media Server
        server: chill-penguin
        container: plex
        widget:
            type: plex
            url: http://plex:32400
            key: "{{ plex_token }}"
            fields: ["streams", "movies", "tv"]
    - Plex Auto Languages:
        icon: plex.png
        href: "#"
        description: Language Manager
        server: chill-penguin
        container: plex-auto-languages
