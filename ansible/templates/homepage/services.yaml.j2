# services
- Calendar:
    - Calendar:
        icon: mdi-calendar
        href: "#"
        description: Release Calendar
        widget:
            type: calendar
            firstDayInWeek: sunday
            view: agenda
            timezone: Pacific/Honolulu
            maxEvents: 20
            integrations:
                - type: sonarr
                  service_group: Library
                  service_name: Sonarr
                  color: blue
                - type: radarr
                  service_group: Library
                  service_name: Radarr
                  color: red

- Plex:
    - Plex:
        icon: plex.png
        href: "#"
        description: Media Server
        server: chill-penguin
        container: plex
        widget:
            type: plex
            url: http://plex:32400
            key: "{{ plex_token }}"
            fields: ["streams", "movies", "tv"]
